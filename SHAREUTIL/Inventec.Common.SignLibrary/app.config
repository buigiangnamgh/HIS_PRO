        <bindings>
            <netTcpBinding>
                <binding name="NetTcpBinding_ISignProcessor">
                    <security mode="Message">
                        <transport sslProtocols="None" />
                    </security>
                </binding>
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://localhost:3081/Inventec/SignProcessorService"
